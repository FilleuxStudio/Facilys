        <section class="section-border pt-50 pb-50">
            <div class="container">
                <div class="row text-center">
                    <div class="col-md-4 mb-4 mb-md-0" style="margin: auto;">
                        <h4 class="mb-15 text-brand">Studio</h4>
                             1188 Av. du Cateau<br>
                             59400 Cambrai, France<br>
                        <abbr title="Phone">Tél : </abbr> (+33) 06 74 52 14 63<br>
                        <abbr title="Email">Email : </abbr><a href="mailto:contact@facilys.fr">contact@facilys.fr</a><br>
                        <a class="btn btn-outline btn-sm btn-brand-outline font-weight-bold text-brand bg-white text-hover-white mt-20 border-radius-5 btn-shadow-brand hover-up" href="https://maps.app.goo.gl/bhStjw2CS6CvS5hN9"><i class="fi-rs-marker mr-10"></i>Voir la Map</a>
                    </div>
                </div>
            </div>
        </section>
        <section class="pt-50 pb-50">
            <div class="container">
                <div class="row">
                    <div class="col-xl-8 col-lg-10 m-auto">
                        <div class="contact-from-area padding-20-row-col wow FadeInUp">
                            <h3 class="mb-10 text-center">Envoyez-nous un message</h3>
                            <p class="text-muted mb-30 text-center font-sm">Ce formulaire permettra de contacter le support de l'application.</p>
                            <% if (message) { %>
                                <p class="form-messege text-success text-center"><%= message %></p>
                            <% } %>
                            <form class="contact-form-style text-center" id="contact-support"  method="POST" action="/contact-support">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12">
                                        <div class="input-style mb-20">
                                            <span class="text-danger f-right">*</span>
                                            <input id="company" name="company" placeholder="Société" type="text" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6">
                                        <div class="input-style mb-20">
                                            <span class="text-danger f-right">*</span>
                                            <input id="name" name="name" placeholder="Nom Complet" type="text" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6">
                                        <div class="input-style mb-20">
                                            <span class="text-danger f-right">*</span>
                                            <input id="email" name="email" placeholder="Votre Email" type="email" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6">
                                        <div class="input-style mb-20">
                                            <span class="text-warning f-right">*</span>
                                            <input id="phone" name="phone" placeholder="Votre Téléphone" type="tel">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6">
                                        <div class="input-style mb-20">
                                            <span class="text-danger f-right">*</span>
                                            <input id="subject" name="subject" placeholder="Sujet" type="text" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-12 col-md-12">
                                        <div class="textarea-style mb-30">
                                            <span class="text-danger f-right">*</span>
                                            <textarea id="message" name="message" placeholder="Message" required></textarea>
                                        </div>
                                        <button class="submit submit-auto-width" type="submit">Envoyer un message</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
