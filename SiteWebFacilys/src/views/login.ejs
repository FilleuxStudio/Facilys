                    <a href="/" rel="nofollow">Boutique</a>
                    <span></span> Pages
                    <span></span> Connexion / S'inscrire
                </div>
            </div>
        </div>
        <section class="pt-150 pb-150">
            <div class="container">
                <div class="row">
                    <div class="col-lg-10 m-auto">
                        <div class="row">
                            <div class="col-lg-5">
                                <div class="login_wrap bg-white widget-taber-content p-30 background-white border-radius-10 mb-md-5 mb-lg-0 mb-sm-5">
                                    <div class="padding_eight_all bg-white">
                                        <div class="heading_s1">
                                            <h3 class="mb-30">Connexion</h3>
                                            <% if (message) { %>
                                                <div class="alert alert-danger">
                                                  <%= message %>
                                                </div>
                                              <% } %>
                                        </div>
                                        <form method="post">
                                            <div class="form-group">
                                                <input type="text" required name="email" id="email" placeholder="Votre Email">
                                            </div>
                                            <div class="form-group">
                                                <input required="" type="password" name="password" id="password" placeholder="Mot de passe">
                                            </div>
                                            <div class="login_footer form-group">
                                                <div class="chek-form">
                                                    <div class="custome-checkbox">
                                                        <input class="form-check-input" type="checkbox" name="remember" id="remember" value="">
                                                        <label class="form-check-label" for="remember"><span>Souvenez-vous de moi</span></label>
                                                    </div>
                                                </div>
                                                <a class="text-muted" href="/reset-password">Mot de passe oublié ?</a>
                                            </div>
                                            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                                            <div class="form-group">
                                                <button type="submit" class="btn btn-fill-out btn-block hover-up btn-brand" name="login">Se connecter</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-1"></div>
                            <div class="col-lg-6">
                                <div class="login_wrap bg-white widget-taber-content p-30 background-white border-radius-5">
                                    <div class="padding_eight_all bg-white">
                                        <div class="heading_s1">
                                            <h3 class="mb-30">S'inscrire</h3>
                                        </div>
                                        <p class="mb-50 font-sm">
                                            Vos données personnelles seront utilisées pour faciliter votre expérience sur ce site web, pour gérer l'accès à votre compte et pour d'autres raisons décrites dans notre politique de confidentialité.
                                        </p>
                                        <form method="POST" action="/register">
                                            <div class="form-group">
                                                <input type="text" required id="companyName" name="companyName" placeholder="Dénomination Société">
                                            </div>
                                            <div class="form-group">
                                                <input type="text" required id="firstName" name="firstName" placeholder="Prénom">
                                            </div>
                                            <div class="form-group">
                                                <input type="text" required id="lastName" name="lastName" placeholder="Nom">
                                            </div>
                                            <div class="form-group">
                                                <input type="text" required id="email" name="email" placeholder="Email">
                                            </div>
                                            <div class="form-group">
                                                <input required="" type="password" id="password" name="password" placeholder="Password">
                                            </div>
                                            <div class="form-group">
                                                <input required="" type="password" id="passwordControl" name="passwordControl" placeholder="Confirm password">
                                            </div>
                                            <div class="login_footer form-group">
                                                <div class="chek-form">
                                                    <div class="custome-checkbox">
                                                        <input class="form-check-input" type="checkbox" name="checkTerm" id="checkTerm">
                                                        <label class="form-check-label" for="checkTerm"><span>j'accepte les conditions &amp; Politique.</span></label>
                                                    </div>
                                                </div>
                                                <a href="/privacy-policy"><i class="fi-rs-book-alt mr-5 text-muted"></i>en savoir plus</a>
                                            </div>
                                            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                                            <div class="form-group">
                                                <button type="submit" class="btn btn-fill-out btn-block hover-up btn-brand" name="register">S'inscrire</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>

